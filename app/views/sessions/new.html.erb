<div class="row">
  <div class="col-3"></div>
  <div class="container col-6">
    <p id="notice"><%= notice %></p>
    <h1>Log In</h1>

    <%= form_tag sessions_path do %>
      <div class="field">
        <%= label_tag :email %><br />
        <%= text_field_tag :email, params[:email] %>
      </div>
      <div class="field">
        <%= label_tag :password %><br />
        <%= password_field_tag :password %>
      </div>
      <br>
      <div class="actions"><%= submit_tag "Log In", :class => "btn btn-default" %></div>
    <% end %>  

  </div>


</div>